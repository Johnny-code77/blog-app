// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Contacts {
  id        String  @id @default(auto()) @map("_id") @db.ObjectId
  fullName  String
  email     String  @unique
  company   String
  message   String
  createdAt DateTime @default(now()) @map("created_at")
}
